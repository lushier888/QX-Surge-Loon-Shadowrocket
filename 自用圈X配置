[general]
# 节点延迟测试链接
server_check_url = http://captive.apple.com/generate_204
# 网络连通性测试链接
network_check_url = http://connectivitycheck.platform.hicloud.com/generate_204
# 测试超时时间 (毫秒)
server_check_timeout = 3000
# 关联配置图标
profile_img_url = https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/softlyx/ZhiPianRen.png
# 节点页面的节点信息展示，可完整自定义展示内容与方式
